        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    if (!gs.nil(current.payload)) {
        var cdm = new IntegrationHandler_CDM();

        var obj = JSON.parse(current.payload);

        try {
            cdm.pushToCustomerAccountImportSet(current);
        } catch (ex) {
            current.status = 'error';
            current.status_message = 'IntegrationHandler_CDM.pushToCustomerAccountImportSet: ' + ex;
			current.update();
			return;
        }

        if (obj.CDMActionType.toLowerCase() == 'accountupdate') {
            try {
                cdm.pushToLocationImportSet(current);
            } catch (ex) {
                current.status = 'error';
                current.status_message = 'IntegrationHandler_CDM.pushToLocationImportSet: ' + ex;
				current.update();
				return;
            }
        }
		
		current.status = 'processed';
		current.update();
    }

})(current, previous);]]></script>
